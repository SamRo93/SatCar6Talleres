<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name = "ordenestrabajo.persistencia.ClienteBanco" tabla ="clientebanco">
        <id column = "id" name = "id" type = "integer">
            <generator class = "increment"/>
        </id>
        <property name="banco"/>
        <property name="codigoPais"/>    
        <property name="titular"/>
        <property name="codCuentaBancaria"/>
        <property name="codDevolucion"/>
        <property name="codIntCuentaBancaria"/>
        <property name="IBAN"/>
        <property name="adeudoSepa"/>
        <property name="tipoSecuencia"/>
        <property name="referenciaMandato"/>
        <property name="fechaMandato"/>
        <property name="bancoAcreedor"/>
    </class>
</hibernate-mapping>



